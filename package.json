{
	"name": "11ty-personal-website",
	"version": "8.0.0",
	"description": "Portfolio and blog, powered by 11ty and Notion",
	"author": "Siddharth Jha <siddharth99c@gmail.com>",
	"license": "MIT",
	"private": true,
	"scripts": {
		"clean": "rm -rf ./.cache",
		"dev": "concurrently \"eleventy --serve\" \"vite\"",
		"build": "mkdir ./.cache && yarn build:vite && yarn build:eleventy",
		"build:eleventy": "NODE_ENV=production eleventy",
		"build:vite": "NODE_ENV=production vite build",
		"prod": "NODE_ENV=production yarn build && serve _site"
	},
	"dependencies": {
		"@11ty/eleventy": "^0.11.0",
		"@11ty/eleventy-cache-assets": "^2.0.4",
		"animejs": "^3.2.1",
		"concurrently": "^6.2.0",
		"dotenv": "^10.0.0",
		"node-fetch": "^2.6.1",
		"rough-notation": "^0.5.1",
		"sass": "^1.35.1",
		"serve": "^12.0.0",
		"spltjs": "^1.1.0",
		"prismjs": "^1.24.1",
		"twemoji": "^13.1.0",
		"vite": "^2.3.8"
	}
}